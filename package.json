{
  "name": "covered-cycle-example",
  "description": "Example Cycle.js counter demo with parts not covered by test coverage removed",
  "version": "0.0.0-semantic-release",
  "author": "Gleb Bahmutov <gleb.bahmutov@gmail.com>",
  "bugs": "https://github.com/bahmutov/covered-cycle-example/issues",
  "config": {
    "pre-git": {
      "commit-msg": "simple",
      "pre-commit": [
        "npm run deps",
        "npm test",
        "npm run ban"
      ],
      "pre-push": [
        "npm run secure",
        "npm run license",
        "npm run ban -- --all",
        "npm run size"
      ],
      "post-commit": [],
      "post-merge": []
    }
  },
  "files": [
    "index.js",
    "src/*.js",
    "!src/*-spec.js"
  ],
  "homepage": "https://github.com/bahmutov/covered-cycle-example#readme",
  "keywords": [
    "code",
    "coverage",
    "cycle",
    "cycle.js",
    "example",
    "istanbul"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bahmutov/covered-cycle-example.git"
  },
  "scripts": {
    "ban": "ban",
    "deps": "deps-ok",
    "format": "standard-format -w index.js src/*.js",
    "issues": "git-issues",
    "license": "license-checker --production --onlyunknown --csv",
    "lint": "standard --verbose index.js src/*.js",
    "pretest": "npm run format && npm run lint",
    "secure": "nsp check",
    "size": "t=\"$(npm pack .)\"; wc -c \"${t}\"; tar tvf \"${t}\"; rm \"${t}\";",
    "test": "npm run unit",
    "unit": "mocha src/*-spec.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "build": "webpack && ls -l dist",
    "server": "http-server",
    "proxy": "rm -rf html-report scripts && was-tested --target http://127.0.0.1:8080"
  },
  "devDependencies": {
    "ban-sensitive-files": "1.8.2",
    "git-issues": "1.2.0",
    "http-server": "0.9.0",
    "license-checker": "5.1.0",
    "mocha": "2.4.5",
    "nsp": "2.3.0",
    "pre-git": "3.8.3",
    "semantic-release": "^4.3.5",
    "standard": "6.0.8",
    "standard-format": "2.1.1",
    "was-tested": "0.10.0",
    "webpack": "1.12.15"
  },
  "dependencies": {
    "@cycle/core": "6.0.3",
    "@cycle/dom": "9.4.0",
    "rx": "4.1.0"
  }
}
